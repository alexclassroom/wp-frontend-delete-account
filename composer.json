{
  "name": "sanzeeb3/wp-frontend-delete-account",
  "description": "Lets customers delete their account by their own.",
  "homepage": "https://github.com/sanzeeb3/wp-frontend-delete-account",
  "type": "wordpress-plugin",
  "license": "GPL-3.0-or-later",
  "require": {
    "composer/installers": "1.7.0"
  },
   "autoload": {
    "files": [
        "includes/Main.php",
        "includes/Backend.php",
        "includes/Frontend.php",
        "includes/WooCommerce.php",
        "includes/Gutenberg.php",
        "includes/Functions.php",
        "includes/emails/Emails.php"
      ]
    },
  "scripts": {
    "phpcs": [
      "phpcs -s -p"
    ],
    "phpcs-pre-commit": [
      "phpcs -s -p -n"
    ],
    "phpcbf": [
      "phpcbf -p"
    ]
  }
}
